xft.dpi:	              	96
xft.hinting:              	slight

Xcursor.theme:            	Comix-WSS
Xcursor.size:             	22

URxvt.font:			xft:MonteCarlo:size=11
URxvt.boldFont:           	xft:MonteCarlo:size=11
URxvt.italicFont:         	-*-montecarlo-medium-*-*-*-11-*-*-*-*-*-*-*
URxvt.boldItalicFont:     	-*-montecarlo-medium-*-*-*-11-*-*-*-*-*-*-*
URxvt.depth:			32
rxvt.title: 				What is thy bidding, my Master

URxvt*geometry:           	160x60
URxvt*saveLines:          	16384
URxvt*scrollBar:		false
URxvt*scrollstyle:	      	plain
!URxvt*scrollBar_right:	  	true
!URxvt*loginShell:	        true
!URxvt*borderLess:        	true
URxvt*tintColor:          	#666666
URxvt*termName:		        rxvt
URxvt*inheritPixmap:	   	false
URxvt*cutchars:		        "\"()*,<>[]{}|'"
!URxvt*secondaryScroll:	  	true
URxvt*perl-lib:		        /usr/lib/urxvt/perl/
URxvt*perl-ext-common:	  	default,matcher
URxvt*urlLauncher:	      	/opt/google/chrome/chrome
URxvt*matcher.button:	    	1
URxvt*matcher.pattern.1:	\\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt*visualBell:	        false

